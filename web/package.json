{
  "name": "Graphene-UI",
  "version": "1.1.0",
  "description": "Advanced wallet interface for graphene based blockchains (Bishares, Muse..)",
  "homepage": "https://github.com/cryptonomex/graphene_ui",
  "author": "Cryptonomex, Inc.",
  "license": "SEE LICENSE IN LICENSE.md",
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cryptonomex/graphene_ui"
  },
  "scripts": {
    "test": "jest",
    "profile-dev": "webpack --config conf/webpack-dev.js --profile --json > stats-dev.json",
    "profile-build": "NODE_ENV=production webpack --config conf/webpack-prod.js --profile --json > stats-prod.json",
    "profile-nougly": "NODE_ENV=production webpack --config conf/webpack-prod-nougly.js --profile --json > stats-prod-nougly.json",
    "start": "node server.js",
    "build": "NODE_ENV=production webpack --config conf/webpack-prod.js --progress --display-error-details --colors",
    "electron": "NODE_ENV=production webpack --config conf/webpack-electron.js --progress --display-error-details --colors;",
    "postelectron": " echo 'copying to electron/build..'; cp -r dist/* ../electron/build/; echo 'done.'",
    "build-win32": "set NODE_ENV=production && webpack --config conf/webpack-prod.js --progress --colors &&  echo 'copying to electron/build..' &&  xcopy dist ..\\electron\\build /s /e /y && echo 'done.'"
  },
  "dependencies": {
    "alt": "^0.17.8",
    "alt-container": "^1.0.0",
    "bignumber.js": "^2.1.0",
    "classnames": "^2.2.1",
    "cookies-js": "^1.2.1",
    "counterpart": "^0.17.1",
    "event-emitter": "^0.3.4",
    "foundation-apps": "git+https://github.com/zurb/foundation-apps.git",
    "git-rev-sync": "^1.4.0",
    "graphenejs-lib": "^0.4.10",
    "highcharts": "4.2.6",
    "history": "1.13.1",
    "html-loader": "^0.4.4",
    "immutable": "^3.8.1",
    "indexeddbshim": "^2.0.4",
    "intl": "^1.1.0",
    "jdenticon": "git+https://github.com/cryptonomex/jdenticon.git",
    "js-sha256": "^0.2.3",
    "lodash": "^3.10.1",
    "lzma": "2.1.6",
    "node-fetch": "^1.3.1",
    "object-assign": "^4.0.1",
    "peerjs": "^0.3.14",
    "perfect-scrollbar": "^0.6.11",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-foundation-apps": "git+https://github.com/valzav/react-foundation-apps.git",
    "react-highcharts": "^10.0.0",
    "react-interpolate-component": "^0.9.0",
    "react-intl": "^2.0.1",
    "react-json-inspector": "^7.0.0",
    "react-motion": "0.3.1",
    "react-notification-system": "^0.2.4",
    "react-popover": "^0.4.4",
    "react-responsive-mixin": "^0.3.5",
    "react-router": "1.0.1",
    "react-tooltip": "^3.0.8",
    "react-translate-component": "^0.11.0",
    "svgo": "^0.5.1",
    "tcomb": "2.5.2",
    "whatwg-fetch": "^0.11.0"
  },
  "devDependencies": {
  	"babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-react-transform": "^2.0.2",
    "clean-webpack-plugin": "^0.1.3",
    "coffee-loader": "^0.7.2",
	"coffee-script": "^1.11.1",
    "css-loader": "^0.22.0",
    "es6-promise": "^3.0.2",
    "eslint": "^2.11.1",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "jest-cli": "~17.0.2",
    "json-loader": "^0.5.3",
    "mocha": "^3.2.0",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.4.1",
    "postcss-loader": "^0.8.2",
    "react-transform-hmr": "^1.0.4",
    "remarkable-loader": "^0.1.0",
    "sass-loader": "^3.1.1",
    "script-loader": "^0.6.1",
    "style-loader": "^0.12.4",
    "svg-inline-loader": "^0.3.0",
    "svgo-loader": "^1.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "worker-loader": "^0.6.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/__tests__/jest-preprocessor.js",
    "testPathDirs": [
      "<rootDir>/__tests__"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/tcomb",
      "<rootDir>/node_modules/immutable"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "coffee"
    ],
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "/jest-preprocessor.js",
      "/node_modules/",
      "/utils/"
    ]
  }
}
